{
  "kind": "collectionType",
  "collectionName": "people",
  "info": {
    "singularName": "person",
    "pluralName": "people",
    "displayName": "People",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string"
    },
    "apellido": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "fecha_nacimiento": {
      "type": "date"
    },
    "genero": {
      "type": "enumeration",
      "enum": [
        "male",
        "female",
        "other"
      ]
    },
    "grupoId": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::grupo.grupo",
      "inversedBy": "peopleId"
    }
  }
}
